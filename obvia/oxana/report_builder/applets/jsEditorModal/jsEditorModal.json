{
  "ctor": "Modal",
  "props": {
    "id": "jsEditorModal",
    "title": "Javascript Expression Editor",
    "components": {
      "modalBody": [
        {
          "ctor": "Container",
          "props": {
            "id": "editorBodyContainer",
            "classes": ["editorBodyContainer"],
            "components": [
              {
                "ctor": "Container",
                "props": {
                  "id": "editorContainer",
                  "components": [
                    {
                      "ctor": "Label",
                      "props": {
                        "id": "editorLabel",
                        "visible": false
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "ctor": "Container",
          "props": {
            "id": "panelContainers",
            "classes": ["panelContainers"],
            "components": [
              {
                "ctor": "Container",
                "props": {
                  "id": "leftPanelContainer",
                  "classes": ["leftPanelContainer"],
                  "components": [
                    {
                      "ctor": "Repeater",
                      "props": {
                        "id": "leftContainerRepeater",
                        "rendering": {
                          "direction": "vertical"
                        },
                        "dataProvider": [],
                        "components": [
                          {
                            "ctor": "Label",
                            "props": {
                              "id": "leftRepeaterLabel",
                              "labelType": "label",
                              "label": "{label}"
                            }
                          }
                        ]
                      }
                    }
                  ]
                }
              },
              {
                "ctor": "Container",
                "props": {
                  "id": "middlePanelContainer",
                  "classes": ["middlePanelContainer"],
                  "components": [
                    {
                      "ctor": "Repeater",
                      "props": {
                        "id": "middleContainerRepeater",
                        "rendering": {
                          "direction": "vertical"
                        },
                        "dataProvider": [],
                        "components": [
                          {
                            "ctor": "Label",
                            "props": {
                              "id": "middleRepeaterLabel",
                              "labelType": "label",
                              "label": "{label}"
                            }
                          }
                        ]
                      }
                    }
                  ]
                }
              },
              {
                "ctor": "Container",
                "props": {
                  "id": "rightPanelContainer",
                  "classes": ["rightPanelContainer"],
                  "components": [
                    {
                      "ctor": "Repeater",
                      "props": {
                        "id": "rightContainerRepeater",
                        "label": "rightContainerRepeater",
                        "rendering": {
                          "direction": "vertical"
                        },
                        "dataProvider": [],
                        "components": [
                          {
                            "ctor": "Label",
                            "props": {
                              "id": "rightRepeaterLabel",
                              "labelType": "label",
                              "label": "{label}"
                            }
                          }
                        ]
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      ],
      "modalFooter": [
        {
          "ctor": "Button",
          "props": {
            "id": "finishButton",
            "classes": ["btn", "btn-primary", "mx-2"],
            "label": "Finish"
          }
        },
        {
          "ctor": "Button",
          "props": {
            "id": "cancelButton",
            "classes": ["btn", "btn-secondary", "mx-2"],
            "label": "Cancel"
          }
        }
      ]
    }
  }
}
