{
    "ctor": "Modal",
    "props": {
      "id": "saveReportModal",
      "title": "Do you want to create a new Report ?",
      "size": "ModalSize.SMALL",
      "components": {
        "modalBody": [
          {
            "ctor": "Container",
            "props": {
              "id": "buttonContainer",
              "css": {
                "width": "100%",
                "display": "flex",
                "justifyContent": "center"
              },
              "components": [
                {
                  "ctor": "Button",
                  "props": {
                    "id": "saveAndCreateNewButton",
                    "classes": ["btn", "btn-secondary", "mx-3"],
                    "label": "Save & Create New"
                  }
                },
                {
                  "ctor": "Button",
                  "props": {
                    "id": "createNewButton",
                    "classes": ["btn", "btn-primary", "mx-3"],
                    "label": "Create New"
                  }
                }
              ]
            }
          }
        ]
      }
    }
  }