{
    "ctor":"Modal",
    "props":{
       "id":"formsModal",
       "size":"ModalSize.LARGE",
       "title":"Select File",
       "components":[
          {
             "ctor":"TextInput",
             "props":{
                "id":"formsFilter",
                "type":"text",
                "placeholder":"Search Forms",
                "classes":[
                   "search-term"
                ]
             }
          },
          {
             "ctor":"Repeater",
             "props":{
                "id":"formsList",
                "rendering":{
                   "direction":"horizontal"
                },
                "components":[
                   {
                      "ctor":"Container",
                      "props":{
                         "id":"cmpForm",
                         "draggable":true,
                         "classes":[
                            "border",
                            "comp_side"
                         ],
                         "css":{
                            "borderRadius":"5px",
                            "margin":"2px 2px"
                         },
                        "width":130,
                        "height":80,
                        "type":"",
                         "components":[
                            {
                               "ctor":"Label",
                               "props":{
                                  "id":"lblForms",
                                  "label":"{form_name}",
                                  "css":{
                                    "fontSize":"15px",
                                    "marginTop":"20px",
                                    "fontWeight":"bold"
                                 }
                               }
                            }
                         ]
                      }
                   }
                ]
             }
          }
       ]
    }
 }