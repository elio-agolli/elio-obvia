{
    "ctor": "Modal",
    "props": {
        "id": "saveForm",
        "size": "ModalSize.LARGE",
        "title": "Save Form",
        "components": {
            "modalFooter": [{
                "ctor": "Button",
                "props": {
                    "id": "saveNew",
                    "label": "Save & New",
                    "classes": ["btn", "btn-primary"]

                }
            }],
            "modalBody": [{
                    "ctor": "ObjectEditor",
                    "props": {
                        "id": "objectEditor",
                        "instance": "{selectedForm}"
                    }
                },
                {
                    "ctor": "Label",
                    "props": {
                        "id": "info",
                        "label": "The form was not saved",
                        "css": {
                            "fontSize": "20px",
                            "color": "red"
                        },
                        "visible": false
                    }
                }
            ]

        }

    }
}