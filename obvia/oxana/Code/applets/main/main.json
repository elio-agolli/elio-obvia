{
    "ctor": "Container",
    "props": {
        "id": "mainContainer",
        "type": "ContainerType.NONE",
        "components": [
            {
                "ctor": "Nav",
                "props": {
                    "id": "mainNav",
                    "height": 40,
                    "classes": [
                        "d-flex"
                    ],
                    "css": {
                        "backgroundColor": "#616161"
                    },
                    "components": [
                        {
                            "ctor": "Select",
                            "props": {
                                "id": "forms",
                                "classes": [
                                    "form-control",
                                    "form-control-sm",
                                    "forms__list"
                                ],
                                "labelField": "form_name",
                                "valueField": "form_guid",
                                "value": 0
                            }
                        },
                        {
                            "ctor": "Select",
                            "props": {
                                "id": "formsComponentsList",
                                "classes": [
                                    "form-control",
                                    "form-control-sm",
                                    "forms__list"
                                ],
                                "css": {
                                    "marginLeft": "15px"
                                },
                                "dataProvider": "{?componentList}",
                                "labelField": "ctor",
                                "valueField": "field_guid",
                                "value": 0
                            }
                        },
                        {
                            "ctor": "Select",
                            "props": {
                                "id": "eventsList",
                                "classes": [
                                    "form-control",
                                    "form-control-sm",
                                    "forms__list"
                                ],
                                "css": {
                                    "marginLeft": "15px"
                                },
                                "dataProvider": "{?eventsList}",
                                "labelField": "eventType",
                                "valueField": "eventId",
                                "value": 0
                            }
                        },
                        {
                            "ctor": "DropEdit",
                            "props": {
                                "id": "behaviorsDropEdit",
                                "classes": [
                                    "behaviors__list"
                                ],
                                "css": {
                                    "marginLeft": "15px"
                                },
                                "hrefField": "form_id",
                                "labelField": "form_name",
                                "label": "Select Behavior"
                            }
                        }
                    ]
                }
            },
            {
                "ctor": "Container",
                "props": {
                    "type": "ContainerType.NONE",
                    "id": "ideContainer",
                    "width": "100%",
                    "height": "100%",
                    "classes": [
                        "d-flex",
                        "flex-shrink-0"
                    ],
                    "components": [
                        {
                            "ctor": "SideNav",
                            "props": {
                                "id": "buttonsSide",
                                "width": 50,
                                "classes": [
                                    "buttons-sideNav"
                                ],
                                "components": [
                                    {
                                        "ctor": "Container",
                                        "props": {
                                            "id": "myCollapseBtn",
                                            "type": "ContainerType.NONE",
                                            "classes": [
                                                "buttons"
                                            ],
                                            "attr": {
                                                "data-toggle": "tooltip",
                                                "data-placement": "right",
                                                "title": "Collapse"
                                            },
                                            "components": [
                                                {
                                                    "ctor": "Link",
                                                    "props": {
                                                        "id": "myCollapseIcon",
                                                        "classes": [
                                                            "fas",
                                                            "fa-bars",
                                                            "bars"
                                                        ],
                                                        "css": {
                                                            "textDecoration": "none"
                                                        }
                                                    }
                                                }
                                            ]
                                        }
                                    },
                                    {
                                        "ctor": "Container",
                                        "props": {
                                            "id": "deployBtn",
                                            "spacing": {
                                                "mr": 1
                                            },
                                            "classes": [
                                                "buttons"
                                            ],
                                            "attr": {
                                                "data-toggle": "tooltip",
                                                "data-placement": "right",
                                                "title": "Deploy"
                                            },
                                            "components": [
                                                {
                                                    "ctor": "Label",
                                                    "props": {
                                                        "id": "fa",
                                                        "labelType": "LabelType.i",
                                                        "classes": [
                                                            "fas",
                                                            "fa-rocket",
                                                            "rocket"
                                                        ]
                                                    }
                                                }
                                            ]
                                        }
                                    },
                                    {
                                        "ctor": "Container",
                                        "props": {
                                            "id": "versionsBtn",
                                            "spacing": {
                                                "mr": 1
                                            },
                                            "classes": [
                                                "buttons"
                                            ],
                                            "attr": {
                                                "data-toggle": "tooltip",
                                                "data-placement": "right",
                                                "title": "Versions"
                                            },
                                            "components": [
                                                {
                                                    "ctor": "Label",
                                                    "props": {
                                                        "id": "fa",
                                                        "labelType": "LabelType.i",
                                                        "classes": [
                                                            "fas",
                                                            "fa-list-ol",
                                                            "list-ol"
                                                        ]
                                                    }
                                                }
                                            ]
                                        }
                                    },
                                    {
                                        "ctor": "Container",
                                        "props": {
                                            "id": "uploadBtn",
                                            "spacing": {
                                                "mr": 1
                                            },
                                            "classes": [
                                                "buttons"
                                            ],
                                            "attr": {
                                                "data-toggle": "tooltip",
                                                "data-placement": "right",
                                                "title": "Upload"
                                            },
                                            "components": [
                                                {
                                                    "ctor": "Label",
                                                    "props": {
                                                        "id": "fa",
                                                        "labelType": "LabelType.i",
                                                        "classes": [
                                                            "fas",
                                                            "fa-cloud-upload-alt",
                                                            "cloud-upload"
                                                        ]
                                                    }
                                                }
                                            ]
                                        }
                                    },
                                    {
                                        "ctor": "Container",
                                        "props": {
                                            "id": "removeBtn",
                                            "spacing": {
                                                "mr": 1
                                            },
                                            "classes": [
                                                "buttons"
                                            ],
                                            "attr": {
                                                "data-toggle": "tooltip",
                                                "data-placement": "right",
                                                "title": "Delete"
                                            },
                                            "components": [
                                                {
                                                    "ctor": "Label",
                                                    "props": {
                                                        "id": "fa",
                                                        "labelType": "LabelType.i",
                                                        "classes": [
                                                            "fas",
                                                            "fa-trash",
                                                            "trash"
                                                        ]
                                                    }
                                                }
                                            ]
                                        }
                                    }
                                ]
                            }
                        },
                        {
                            "ctor": "SideNav",
                            "props": {
                                "id": "mySideNav",
                                "width": 200,
                                "classes": [
                                    "sidenav",
                                    "height"
                                ],
                                "css": {
                                    "backgroundColor": "#616161",
                                    "paddingTop": "20px"
                                },
                                "components": [
                                    {
                                        "ctor": "Container",
                                        "props": {
                                            "id": "cmCnt",
                                            "components": [
                                                {
                                                    "ctor": "Link",
                                                    "props": {
                                                        "id": "cmCollapse",
                                                        "attr": {
                                                            "data-toggle": "collapse"
                                                        },
                                                        "label": "Component Model",
                                                        "classes": [
                                                            "collapse_icon",
                                                            "component__model"
                                                        ],
                                                        "css": {
                                                            "color": "#fff"
                                                        },
                                                        "components": [
                                                            {
                                                                "ctor": "Label",
                                                                "props": {}
                                                            }
                                                        ]
                                                    }
                                                },
                                                {
                                                    "ctor": "Tree",
                                                    "props": {
                                                        "id": "componentModelTree",
                                                        "valueField": "guid",
                                                        "labelField": "label",
                                                        "childrenField": "children",
                                                        "expandIcon": "fa-chevron-circle-right",
                                                        "collapseIcon": "fa-chevron-circle-down",
                                                        "iconField": "iconClasses",
                                                        "classes": [
                                                            "collapse",
                                                            "list-unstyled"
                                                        ],
                                                        "selectedClasses": [],
                                                        "ulClasses": [
                                                            "d-none",
                                                            "list-unstyled",
                                                            "ml-1"
                                                        ],
                                                        "afterAttach": "_bindCmCollapsible"
                                                    }
                                                }
                                            ]
                                        }
                                    },
                                    {
                                        "ctor": "Container",
                                        "props": {
                                            "id": "todosCnt",
                                            "components": [
                                                {
                                                    "ctor": "Link",
                                                    "props": {
                                                        "id": "todosCollapse",
                                                        "attr": {
                                                            "data-toggle": "collapse"
                                                        },
                                                        "label": "TODOs",
                                                        "classes": [
                                                            "collapse_icon"
                                                        ],
                                                        "css": {
                                                            "color": "#fff"
                                                        },
                                                        "components": [
                                                            {
                                                                "ctor": "Label",
                                                                "props": {}
                                                            }
                                                        ]
                                                    }
                                                },
                                                {
                                                    "ctor": "Repeater",
                                                    "props": {
                                                        "id": "todosRepeater",
                                                        "rendering": {
                                                            "direction": "vertical"
                                                        },
                                                        "components": [
                                                            {
                                                                "ctor": "Link",
                                                                "props": {
                                                                    "id": "todoItem",
                                                                    "label": "{comment}",
                                                                    "href": "#",
                                                                    "target": "",
                                                                    "click": "todoItemClick"
                                                                }
                                                            }
                                                        ],
                                                        "classes": [
                                                            "collapse"
                                                        ],
                                                        "afterAttach": "_bindCollapsible"
                                                    }
                                                }
                                            ]
                                        }
                                    }
                                ]
                            }
                        },
                        {
                            "ctor": "Code",
                            "props": {
                                "id": "myCode",
                                "type": "ContainerType.NONE",
                                "height": "calc(100vh - 50px) !important",
                                "endDraw": "_focusEditor"
                            }
                        }
                    ]
                }
            }
        ]
    }
}